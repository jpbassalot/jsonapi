{
    "name": "jpbassalot/jsonapi",
    "description": "A simple JSON API to read and paginate JSON files in a directory",
    "type": "library",
    "keywords": [
        "json",
        "api",
        "jsonapi",
        "json-api",
        "json api"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1"
    },

    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "fakerphp/faker": "^1.21"
    },
    "autoload": {
        "psr-4": {
            "Jpbassalot\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jpbassalot\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Joseph C White",
            "email": "joseph.white@kccd.edu"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true
}
